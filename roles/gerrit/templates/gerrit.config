[gerrit]
        basePath = git
        canonicalWebUrl = {{gerrit_url}}
[database]
        type = POSTGRESQL
        hostname = localhost
        database = {{db_name}}
        username = {{db_user}}
        password = {{db_password}}
[index]
        type = LUCENE
[auth]
        type = OPENID
[sendemail]
        smtpServer = {{database_server}}
[container]
        user = {{unix_user}}
        javaHome = /usr/lib/jvm/java-1.7.0-openjdk-1.7.0.91-2.6.2.1.el7_1.x86_64/jre
[sshd]
        listenAddress = *:29418
[httpd]
        listenUrl = http://*:8080/
[cache]
        directory = cache
